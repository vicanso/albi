script.
  (function(global){
    global.CONFIG = {
      debug: #{DEBUG || false},
      env: '#{ENV}',
      staticUrlPrefix: '#{STATIC_URL_PREFIX}',
      appUrlPrefix: '#{APP_URL_PREFIX}',
      appVersion: '#{APP_VERSION}',
      template: '#{TEMPLATE}',
      startAt: Date.now()
    };
    var TIMING = {
      data: {},
      startTimes: {},
      start: function(tag){
        this.startTimes[tag] = new Date().getTime();
        return this;
      },
      end: function(tag, startTag){
        var startTimes = this.startTimes;
        var start = startTimes[tag] || startTimes[startTag];
        if(start){
          this.data[tag] = new Date().getTime() - start;
        }
        return this;
      },
      toJSON : function(){
        return this.data;
      }
    };
    TIMING.start('html');
    TIMING.start('page');
    global.TIMING = TIMING;
    var TMP = {
      errors: [],
      resources: [],
      resourceLoadErrors: []
    };
    global.TMP = TMP;
    global.onerror = function(msg, url , line, row, err){
      var stack = '';
      if(err){
        stack = err.stack;
      }
      var data = {
        url: url,
        line: line,
        row: row,
        msg: msg,
        stack: stack
      };
      if(CONFIG.env === 'development'){
        alert(JSON.stringify(data));
      }
      TMP.errors.push(data);
    };
  })(this);
