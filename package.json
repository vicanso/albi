{
  "name": "albi",
  "description": "backend framework base on koa",
  "version": "3.0.0",
  "author": "Tree Xie <vicansocanbico@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/vicanso/albi"
  },
  "bugs": {
    "url": "https://github.com/vicanso/albi/issues",
    "email": "vicansocanbico@gmail.com"
  },
  "main": "./app",
  "keywords": [
    "koa",
    "framework",
    "albi"
  ],
  "engines": {
    "node": ">=6.0.0"
  },
  "readmeFilename": "README.md",
  "scripts": {
    "test": "node ./node_modules/.bin/eslint *.js helpers/*.js middlewares/*.js router/*.js controllers/*.js && NODE_ENV=test INFLUX=http://127.0.0.1:8086/mydb node ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha test/*.js test/helpers/*.js test/middlewares/*.js --",
    "install-all": "npm i && node node_modules/.bin/jspm install"
  },
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.3.5",
    "bytes": "^2.3.0",
    "debug": "^2.2.0",
    "http-errors": "^1.4.0",
    "influxdb-nodejs": "^1.0.0",
    "joi": "^8.1.0",
    "jtfileimporter": "^0.4.2",
    "koa": "^2.0.0",
    "koa-bodyparser": "^3.1.0",
    "koa-connection-limit": "^2.0.4",
    "koa-convert": "^1.2.0",
    "koa-etag": "^3.0.0",
    "koa-fresh": "0.0.3",
    "koa-http-stats": "^2.1.7",
    "koa-log": "^2.0.0",
    "koa-methodoverride": "^2.0.0",
    "koa-mounting": "^2.0.3",
    "koa-query-checker": "^2.0.1",
    "koa-rest-version": "0.0.3",
    "koa-router-parser": "^2.0.3",
    "koa-static-serve": "^2.0.5",
    "lodash": "^4.12.0",
    "micro-service": "0.0.4",
    "performance-nodejs": "0.0.5",
    "pug": "^2.0.0-alpha7",
    "require-tree": "^1.1.1",
    "superagent": "^2.0.0-alpha.2",
    "toobusy-js": "^0.5.0",
    "url-join": "^1.1.0"
  },
  "devDependencies": {
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.7.0",
    "istanbul": "^0.4.3",
    "jspm": "^0.16.34",
    "koa-stylus-parser": "^2.0.0",
    "mocha": "^2.4.5",
    "supertest": "^1.2.0"
  },
  "jspm": {
    "directories": {
      "baseURL": "jspm",
      "packages": "jspm/packages"
    },
    "dependencies": {
      "debug": "npm:debug@^2.2.0",
      "lodash": "npm:lodash@^4.12.0",
      "superagent": "npm:superagent@^1.8.2",
      "uuid": "npm:uuid@^2.0.2"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    }
  }
}
