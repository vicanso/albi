{
  "name": "albi",
  "description": "backend framework base on koa",
  "version": "4.0.0",
  "author": "Tree Xie <vicansocanbico@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/vicanso/albi"
  },
  "bugs": {
    "url": "https://github.com/vicanso/albi/issues",
    "email": "vicansocanbico@gmail.com"
  },
  "main": "./app",
  "keywords": [
    "koa",
    "framework",
    "albi"
  ],
  "engines": {
    "node": ">=6.0.0"
  },
  "readmeFilename": "README.md",
  "scripts": {
    "eslint-es6": "node ./node_modules/.bin/eslint public/js/*.js public/js/*/*.js",
    "eslint-node": "node ./node_modules/.bin/eslint *.js helpers/*.js middlewares/*.js router/*.js controllers/*.js models/*.js",
    "eslint": "npm run eslint-node && npm run eslint-es6",
    "coverage": "node ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha test/*.js test/controllers/*.js test/helpers/*.js test/middlewares/*.js test/services/*.js --",
    "test": "npm run eslint && NODE_ENV=test INFLUX=http://127.0.0.1:8086/mydb MONGO=mongodb://127.0.0.1/albi REDIS=redis://127.0.0.1:6379 npm run coverage",
    "install-all": "npm i && node node_modules/.bin/jspm install",
    "build": "NODE_ENV=production node ./node_modules/.bin/gulp",
    "watch": "node node_modules/.bin/webpack --progress --colors --watch",
    "start": "node app",
    "dev": "MONGO=mongodb://127.0.0.1/albi nodemon app"
  },
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.4.6",
    "classnames": "^2.2.5",
    "debug": "^2.3.0",
    "etcd-register": "1.0.0",
    "http-errors": "^1.5.0",
    "influxdb-nodejs": "^2.1.1",
    "joi": "^9.2.0",
    "jtfileimporter": "^0.4.2",
    "koa": "^2.0.0",
    "koa-bodyparser": "^3.2.0",
    "koa-connection-limit": "^2.0.5",
    "koa-convert": "^1.2.0",
    "koa-etag": "^3.0.0",
    "koa-fresh": "0.0.3",
    "koa-http-stats": "^2.1.8",
    "koa-log": "^2.0.0",
    "koa-methodoverride": "^2.0.0",
    "koa-mounting": "^2.0.4",
    "koa-query-checker": "^2.0.3",
    "koa-rest-version": "^1.0.0",
    "koa-router-parser": "^3.0.1",
    "koa-simple-redis": "^1.0.2",
    "koa-simple-session": "^1.0.2",
    "koa-static-serve": "^2.1.3",
    "koa-stylus-parser": "^2.0.2",
    "lodash": "^4.16.6",
    "moment": "^2.15.2",
    "mongoose": "^4.6.6",
    "performance-nodejs": "^1.0.2",
    "pug": "^2.0.0-beta6",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-enroute": "^1.0.0",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.0",
    "redux-thunk": "^2.1.0",
    "require-tree": "^1.1.1",
    "simple-stringify": "^1.0.0",
    "superagent": "^2.3.0",
    "url-join": "^1.1.0",
    "uuid": "^3.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "buffer-crc32": "^0.2.5",
    "del": "^2.2.2",
    "eslint": "^3.7.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "gulp": "^3.9.1",
    "gulp-base64": "^0.1.3",
    "gulp-copy": "0.0.2",
    "gulp-cssmin": "^0.1.7",
    "gulp-stylus": "^2.5.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.1.2",
    "nib": "^1.1.2",
    "supertest": "^2.0.0",
    "through2": "^2.0.1",
    "webpack": "^1.13.2"
  }
}
