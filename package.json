{
  "name": "albi",
  "description": "backend framework base on koa",
  "version": "5.0.0",
  "author": "Tree Xie <vicansocanbico@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/vicanso/albi"
  },
  "bugs": {
    "url": "https://github.com/vicanso/albi/issues",
    "email": "vicansocanbico@gmail.com"
  },
  "main": "./app",
  "keywords": [
    "koa",
    "framework",
    "albi"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "readmeFilename": "README.md",
  "scripts": {
    "size": "t=\"$(npm pack .)\"; wc -c \"${t}\"; tar tvf \"${t}\"; rm \"${t}\";",
    "eslint-es6": "node ./node_modules/.bin/eslint public/js/*.js public/js/*/*.js",
    "eslint-node": "node ./node_modules/.bin/eslint *.js helpers/*.js middlewares/*.js router/*.js controllers/*.js models/*.js services/*.js",
    "eslint": "npm run eslint-node && npm run eslint-es6",
    "coverage": "node ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha test/*.js test/controllers/*.js test/helpers/*.js test/middlewares/*.js --",
    "test": "npm run eslint && NODE_ENV=test INFLUX=http://127.0.0.1:8086/mydb MONGO=mongodb://127.0.0.1/albi REDIS=redis://127.0.0.1:6379 npm run coverage",
    "build": "NODE_ENV=production node ./node_modules/.bin/gulp",
    "docs": "node ./node_modules/.bin/jsdoc -c .jsdoc.json --readme ./README.md",
    "watch": "node node_modules/.bin/webpack --progress --colors --watch",
    "start": "node app",
    "dev": "nodemon app"
  },
  "license": "MIT",
  "dependencies": {
    "async-local-storage": "^1.0.0",
    "bluebird": "^3.5.0",
    "classnames": "^2.2.5",
    "debug": "^2.6.8",
    "etcd-register": "1.0.0",
    "http-errors": "^1.6.1",
    "http-timing": "^1.0.1",
    "influxdb-nodejs": "^2.7.2",
    "ioredis": "^2.5.0",
    "joi": "^10.5.2",
    "jtfileimporter": "^1.0.0",
    "koa": "^2.2.0",
    "koa-bodyparser": "^4.2.0",
    "koa-connection-limit": "^2.1.0",
    "koa-etag": "^3.0.0",
    "koa-http-stats": "^2.2.1",
    "koa-log": "^2.1.0",
    "koa-methodoverride": "^2.0.0",
    "koa-mounting": "^2.0.4",
    "koa-query-checker": "^2.0.3",
    "koa-rest-version": "^1.0.0",
    "koa-router-parser": "^3.1.1",
    "koa-session": "^5.1.0",
    "koa-static-serve": "^2.1.3",
    "koa-stylus-parser": "^2.0.2",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "mongoose": "^4.10.5",
    "ms": "^1.0.0",
    "performance-nodejs": "^1.1.0",
    "pug": "^2.0.0-rc.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-enroute": "^1.0.0",
    "react-redux": "^5.0.5",
    "redux": "^3.6.0",
    "redux-logger": "^2.10.2",
    "redux-thunk": "^2.2.0",
    "require-tree": "^1.1.1",
    "shortid": "^2.2.8",
    "simple-stringify": "^1.1.1",
    "superagent": "^3.5.2",
    "supertiming": "^1.3.0",
    "timtam-logger": "^2.0.1",
    "url-join": "^2.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "crc": "^3.4.4",
    "del": "^2.2.2",
    "docdash": "^0.4.0",
    "eslint": "^3.17.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "gulp": "^3.9.1",
    "gulp-base64": "^0.1.3",
    "gulp-copy": "1.0.0",
    "gulp-cssmin": "^0.1.7",
    "gulp-stylus": "^2.6.0",
    "istanbul": "^1.1.0-alpha.1",
    "jsdoc": "^3.4.3",
    "mocha": "^3.2.0",
    "nib": "^1.1.2",
    "supertest": "^3.0.0",
    "through2": "^2.0.3",
    "webpack": "^1.15.0"
  }
}
