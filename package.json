{
  "name": "albi",
  "description": "backend framework base on koa",
  "version": "3.0.0",
  "author": "Tree Xie <vicansocanbico@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/vicanso/albi"
  },
  "bugs": {
    "url": "https://github.com/vicanso/albi/issues",
    "email": "vicansocanbico@gmail.com"
  },
  "main": "./app",
  "keywords": [
    "koa",
    "framework",
    "albi"
  ],
  "engines": {
    "node": ">=6.0.0"
  },
  "readmeFilename": "README.md",
  "scripts": {
    "eslint-jspm": "node ./node_modules/.bin/eslint jspm/bootstrap.js jspm/store.js jspm/components/*.js jspm/actions/*.js jspm/constants/*.js jspm/reducers/*.js jspm/services/*.js",
    "eslint-node": "node ./node_modules/.bin/eslint *.js helpers/*.js middlewares/*.js router/*.js controllers/*.js models/*.js",
    "eslint": "npm run eslint-node && npm run eslint-jspm",
    "coverage": "node ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha test/*.js test/controllers/*.js test/helpers/*.js test/middlewares/*.js test/services/*.js --",
    "test": "npm run eslint && NODE_ENV=test INFLUX=http://127.0.0.1:8086/mydb MONGO=mongodb://127.0.0.1/albi REDIS=redis://127.0.0.1:6379 npm run coverage",
    "install-all": "npm i && node node_modules/.bin/jspm install",
    "gulp": "node ./node_modules/.bin/gulp",
    "start": "node app",
    "dev": "MONGO=mongodb://127.0.0.1/albi nodemon app"
  },
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.4.0",
    "bytes": "^2.4.0",
    "debug": "^2.2.0",
    "http-errors": "^1.5.0",
    "influxdb-nodejs": "^1.0.5",
    "joi": "^9.0.0-5",
    "jtfileimporter": "^0.4.2",
    "koa": "^2.0.0",
    "koa-bodyparser": "^3.1.0",
    "koa-connection-limit": "^2.0.4",
    "koa-convert": "^1.2.0",
    "koa-etag": "^3.0.0",
    "koa-fresh": "0.0.3",
    "koa-http-stats": "^2.1.7",
    "koa-log": "^2.0.0",
    "koa-methodoverride": "^2.0.0",
    "koa-mounting": "^2.0.3",
    "koa-query-checker": "^2.0.2",
    "koa-rest-version": "0.0.3",
    "koa-router-parser": "^3.0.1",
    "koa-simple-redis": "0.0.2",
    "koa-simple-session": "0.0.1",
    "koa-static-serve": "^2.0.5",
    "lodash": "^4.13.1",
    "micro-service": "0.0.4",
    "mongoose": "^4.5.0",
    "performance-nodejs": "0.0.5",
    "pug": "^2.0.0-beta3",
    "require-tree": "^1.1.1",
    "superagent": "^2.0.0",
    "url-join": "^1.1.0"
  },
  "devDependencies": {
    "buffer-crc32": "^0.2.5",
    "del": "^2.2.0",
    "eslint": "^2.12.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.4.2",
    "eslint-plugin-react": "^5.1.1",
    "gulp": "^3.9.1",
    "gulp-base64": "^0.1.3",
    "gulp-copy": "0.0.2",
    "gulp-cssmin": "^0.1.7",
    "gulp-shell": "^0.5.2",
    "gulp-stylus": "^2.3.1",
    "istanbul": "^0.4.3",
    "jspm": "^0.16.34",
    "koa-stylus-parser": "^2.0.0",
    "mocha": "^2.4.5",
    "nib": "^1.1.0",
    "supertest": "^1.2.0",
    "through2": "^2.0.1"
  },
  "jspm": {
    "directories": {
      "baseURL": "jspm",
      "packages": "jspm/packages"
    },
    "dependencies": {
      "classnames": "npm:classnames@^2.2.5",
      "debug": "npm:debug@^2.2.0",
      "lodash": "npm:lodash@^4.12.0",
      "react": "npm:react@^15.1.0",
      "react-dom": "npm:react-dom@^15.1.0",
      "react-redux": "npm:react-redux@^4.4.5",
      "redux": "npm:redux@^3.5.2",
      "redux-logger": "npm:redux-logger@^2.6.1",
      "redux-thunk": "npm:redux-thunk@^2.1.0",
      "superagent": "npm:superagent@^1.8.2",
      "uuid": "npm:uuid@^2.0.2"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    }
  }
}
