{
  "name": "albi",
  "description": "backend framework base on koa",
  "version": "6.0.0",
  "author": "Tree Xie <vicansocanbico@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/vicanso/albi"
  },
  "bugs": {
    "url": "https://github.com/vicanso/albi/issues",
    "email": "vicansocanbico@gmail.com"
  },
  "main": "./app",
  "keywords": [
    "koa",
    "framework",
    "albi"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "readmeFilename": "README.md",
  "scripts": {
    "coverage": "node ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha test/*.js test/*/*.js --",
    "dev": "nodemon app",
    "eslint": "npm run eslint-node & npm run eslint-browser",
    "eslint-browser": "node ./node_modules/.bin/eslint public/js/*/*.js[x] public/js/*.js",
    "eslint-node": "node ./node_modules/.bin/eslint *.js test/*.js test/*/*.js configs/*.js helpers/*.js errors/*.js schedules/*.js middlewares/*.js influx-schemas/*.js models/*.js routes/*.js controllers/*.js services/*.js plugins/*.js",
    "size": "t=\"$(npm pack .)\"; wc -c \"${t}\"; tar tvf \"${t}\"; rm \"${t}\";",
    "start": "node app",
    "test": "npm run eslint && NODE_ENV=test INFLUX=http://127.0.0.1:8086/albi npm run coverage",
    "watch": "node node_modules/.bin/webpack --progress --colors --watch"
  },
  "license": "MIT",
  "dependencies": {
    "async-local-storage": "^1.0.1",
    "bluebird": "^3.5.0",
    "debug": "^2.6.8",
    "http-errors": "^1.6.1",
    "influxdb-nodejs": "^2.7.2",
    "ioredis": "^3.1.1",
    "joi": "^10.6.0",
    "jtfileimporter": "^1.0.0",
    "koa": "^2.3.0",
    "koa-bodyparser": "^4.2.0",
    "koa-connection-limit": "^2.1.0",
    "koa-etag": "^3.0.0",
    "koa-http-stats": "^2.2.1",
    "koa-log": "^2.1.0",
    "koa-methodoverride": "^2.0.0",
    "koa-mounting": "^2.0.4",
    "koa-rest-version": "^1.0.0",
    "koa-router-parser": "^3.1.1",
    "koa-session": "^5.4.0",
    "koa-static-serve": "^2.1.3",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "mongoose": "^4.11.1",
    "ms": "^2.0.0",
    "performance-nodejs": "^1.1.3",
    "pug": "^2.0.0-rc.2",
    "require-tree": "^1.1.1",
    "shortid": "^2.2.8",
    "simple-stringify": "^1.1.2",
    "superagent": "^3.5.2",
    "supertiming": "^1.3.1",
    "timtam-logger": "^2.0.1",
    "url-join": "^2.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.28.4",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.0.1",
    "http-timing": "^1.0.2",
    "istanbul": "^1.1.0-alpha.1",
    "material-ui": "^1.0.0-alpha.21",
    "material-ui-icons": "^1.0.0-alpha.19",
    "mocha": "^3.4.2",
    "postcss-cssnext": "^3.0.2",
    "postcss-loader": "^2.0.6",
    "prop-types": "^15.5.10",
    "query-string": "^4.3.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.2",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "style-loader": "^0.18.2",
    "sugarss": "^1.0.0",
    "webpack": "^3.2.0"
  }
}
