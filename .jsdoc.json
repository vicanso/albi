{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include": ["./helpers", "./errors", "./influx-schemas"],
    "includePattern": ".js$"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "encoding": "utf8",
    "destination": "js-docs/",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "docdash": {
    "static": true
  }
}
