{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include": [
      "./helpers",
      "./errors",
      "./influx-schemas",
      "./controllers",
      "./middlewares"
    ],
    "includePattern": ".js$"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "template": "node_modules/docdash",
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "docdash": {
    "static": true
  }
}
